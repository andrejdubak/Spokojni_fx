<?xml version="1.0" encoding="UTF-8"?>

<?import com.calendarfx.view.CalendarView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Font?>
<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.spokojni.frontend.StudentViewController">
    <children>
        <CalendarView fx:id="calendarView" prefHeight="760.0" prefWidth="750.0" showAddCalendarButton="false" showPrintButton="false" />
        <Button mnemonicParsing="false" onAction="#buttonClick" prefHeight="26.0" prefWidth="160.0" text="%Load_calendar" wrapText="true">
            <font>
                <Font size="16.0" />
            </font></Button>
        <Button mnemonicParsing="false" onAction="#saveClick" prefHeight="26.0" prefWidth="160.0" text="%Save_and_exit" wrapText="true">
            <font>
                <Font size="16.0" />
            </font></Button>
    </children>
</VBox>
