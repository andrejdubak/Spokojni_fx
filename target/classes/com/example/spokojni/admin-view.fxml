                <TableColumn fx:id="emailTable" prefWidth="165.0" text="Email" />
                <TableColumn fx:id="roleTable" prefWidth="119.0" text="%Role" />
            </columns>
        </TableView>
        <Button fx:id="Profile" layoutX="565.0" layoutY="50.0" mnemonicParsing="false" onAction="#ProfileClick" prefHeight="75.0" prefWidth="149.0" text="%Profile" textAlignment="CENTER" />
