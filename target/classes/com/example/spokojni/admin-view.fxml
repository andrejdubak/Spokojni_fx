        <TextField fx:id="search" layoutX="348.0" layoutY="424.0" prefHeight="35.0" prefWidth="149.0" promptText="%Search" />


        <Text layoutX="20.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%Welcome" wrappingWidth="100.0">
            <font>
                <Font size="17.0" />
            </font>
        </Text>
        <Text fx:id="name" layoutX="110.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin" wrappingWidth="92.30078125">
            <font>
                <Font size="16.0" />
            </font>
        </Text>
      <CheckBox fx:id="showS" layoutX="26.0" layoutY="433.0" mnemonicParsing="false" onAction="#loadUsers" text="%Students" />
      <CheckBox fx:id="showT" layoutX="122.0" layoutY="433.0" mnemonicParsing="false" onAction="#loadUsers" text="%Teachers" />
    </children>
