<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<DialogPane prefHeight="430.0" prefWidth="311.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.spokojni.frontend.SubjectsController">
   <content>
      <AnchorPane prefHeight="278.0" prefWidth="311.0">
         <children>
            <TextField fx:id="subject_name" alignment="CENTER" layoutX="128.0" layoutY="310.0" prefHeight="25.0" prefWidth="207.0" promptText="subject name" />
            <Button fx:id="add_subject" layoutX="193.0" layoutY="355.0" mnemonicParsing="false" onAction="#addSubject" text="add subject" />
            <TableView fx:id="table" layoutX="19.0" layoutY="14.0" prefHeight="274.0" prefWidth="438.0">
              <columns>
                <TableColumn fx:id="table_column" prefWidth="436.6666660308838" text="C1" />
              </columns>
            </TableView>
            <Button fx:id="load_subject" layoutX="369.0" layoutY="297.0" mnemonicParsing="false" onAction="#loadSubjects" text="Load subjects" />
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
</DialogPane>
